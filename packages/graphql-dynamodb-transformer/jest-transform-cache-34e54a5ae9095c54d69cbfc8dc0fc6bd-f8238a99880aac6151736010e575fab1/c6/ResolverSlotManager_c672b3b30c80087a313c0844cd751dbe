"use strict";

Object.defineProperty(exports, "__esModule", {
  value: true
});
exports.ResolverSlotManager = void 0;

class ResolverSlotManager {
  constructor(slotsNames) {
    this.slotsNames = slotsNames;
    this.slots = new Map();
    slotsNames.forEach(name => {
      this.slots.set(name, []);
    });
  }

  addTo(slotName, content) {
    if (!this.slotsNames.includes(slotName)) {
      throw new Error(`Missing slot ${slotName}`);
    }

    const slot = this.slots.get(slotName);
    slot.push(content);
  }

  get(name) {
    return this.slots.get(name);
  }

}

exports.ResolverSlotManager = ResolverSlotManager;
